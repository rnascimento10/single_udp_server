{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.Console" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "D:\\Logs\\log-.log",
          "rollingInterval": "Hour"
        }
      },
      {
        "Name": "Console"
      }
    ]
  },


  "Listener": {
    "IpAddress": "0.0.0.0",
    "Ports": [ "5198" ]
  }


}
